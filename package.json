{
  "name": "rapid.js",
  "version": "1.1.3",
  "description": "",
  "main": "dist/rapid.js",
  "scripts": {
    "test": "npm run unit && npm run lint",
    "compile": "babel --presets=es2015 -d dist src",
    "copy-dev": "npm run compile && rm -rf ./demo/rapid && cp -R ./dist ./demo/rapid",
    "prepublish": "npm run compile",
    "lint": "eslint --ext .js src test",
    "unit": "jest --maxWorkers 2",
    "unit:watch": "jest test/**/*.test.js --watch",
    "build": "babel src --out-dir lib --extensions \".ts,.tsx\"",
    "type-check": "tsc"
  },
  "babel": {
    "presets": [
      "es2015",
      "@babel/env",
      "@babel/typescript"
    ],
    "plugins": [
      "@babel/proposal-class-properties",
      "@babel/proposal-object-rest-spread"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rapidjs/rapid.js.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rapidjs/rapid.js/issues"
  },
  "homepage": "https://github.com/rapidjs/rapid.js#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.32",
    "@babel/core": "^7.0.0-beta.32",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.32",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.32",
    "@babel/preset-env": "^7.0.0-beta.32",
    "@babel/preset-typescript": "^7.0.0-beta.32",
    "@types/jest": "^22.1.3",
    "babel-eslint": "^8.0.2",
    "babel-preset-es2015": "^6.24.0",
    "eslint": "^4.11.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-airbnb-standard": "^1.6.5",
    "eslint-plugin-import": "^2.8.0",
    "jest": "^21.2.1",
    "ts-jest": "^22.4.0",
    "typescript": "^2.7.2"
  },
  "dependencies": {
    "axios": "^0.16.0",
    "lodash": "^4.17.4",
    "pluralize": "^4.0.0",
    "qs": "^6.4.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
