{
  "name": "rapid.js",
  "version": "1.2.0",
  "description": "",
  "main": "dist/rapid.js",
  "scripts": {
    "test": "npm run unit && npm run lint",
    "compile": "./node_modules/.bin/babel --presets=@babel/preset-es2015 -d dist src",
    "copy-dev": "npm run compile && rm -rf ./demo/rapid && cp -R ./dist ./demo/rapid",
    "prepublish": "npm run compile",
    "lint": "./node_modules/.bin/eslint --ext .js,.ts src test",
    "lint:fix": "./node_modules/.bin/eslint --ext .js,.ts src test --fix",
    "unit": "./node_modules/.bin/jest test/auth.test.ts --maxWorkers 2",
    "unit:watch": "./node_modules/.bin/jest test/**/*.test.ts --watch",
    "build": "./node_modules/.bin/babel src --out-dir dist --extensions \".ts,.tsx\"",
    "ts": "./node_modules/.bin/tsc"
  },
  "babel": {
    "presets": [
      "@babel/env",
      "@babel/typescript"
    ],
    "plugins": [
      "@babel/proposal-class-properties",
      "@babel/proposal-object-rest-spread"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rapidjs/rapid.js.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rapidjs/rapid.js/issues"
  },
  "homepage": "https://github.com/rapidjs/rapid.js#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.44",
    "@babel/core": "^7.0.0-beta.44",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.32",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.32",
    "@babel/preset-env": "^7.0.0-beta.32",
    "@babel/preset-es2015": "^7.0.0-beta.44",
    "@babel/preset-typescript": "^7.0.0-beta.32",
    "@types/jest": "^22.1.4",
    "babel-eslint": "^8.0.2",
    "eslint": "^4.11.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-airbnb-standard": "^1.6.5",
    "eslint-plugin-import": "^2.8.0",
    "jest": "^21.2.1",
    "ts-jest": "^22.4.1",
    "tslint-config-airbnb": "^5.7.0",
    "typescript": "^2.7.2",
    "typescript-eslint-parser": "^14.0.0"
  },
  "dependencies": {
    "axios": "^0.16.0",
    "lodash": "^4.17.4",
    "pluralize": "^4.0.0",
    "qs": "^6.4.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "testRegex": "(/__test__/.*|(\\.|/)(test|spec))\\.(tsx?|ts?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
