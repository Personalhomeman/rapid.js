{
  "name": "rapid.js",
  "version": "1.2.0",
  "description": "",
  "main": "dist/rapid.js",
  "types": "dist/rapid.d.ts",
  "scripts": {
    "test": "npm run unit && npm run lint",
    "compile": "babel --presets=@babel/preset-env -d dist src",
    "copy-dev": "npm run compile && rm -rf ./demo/rapid && cp -R ./dist ./demo/rapid",
    "prepublish": "npm run compile",
    "lint": "eslint --ext .js,.ts src test",
    "lint:fix": "eslint --ext .js,.ts src test --fix",
    "unit": "jest test/auth.test.ts --maxWorkers 2",
    "unit:watch": "jest test/**/*.test.ts --watch",
    "build": "babel src --out-dir dist --extensions \".ts,.tsx\"",
    "ts": "tsc"
  },
  "babel": {
    "presets": [
      "@babel/env",
      "@babel/typescript"
    ],
    "plugins": [
      "@babel/proposal-class-properties",
      "@babel/proposal-object-rest-spread"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rapidjs/rapid.js.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rapidjs/rapid.js/issues"
  },
  "homepage": "https://github.com/rapidjs/rapid.js#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.44",
    "@babel/core": "^7.0.0-beta.44",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.32",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.32",
    "@babel/preset-env": "^7.0.0-beta.32",
    "@babel/preset-typescript": "^7.0.0-beta.32",
    "@types/jest": "^22.1.4",
    "babel-eslint": "^8.0.2",
    "@typescript-eslint/eslint-plugin": "1.4.2",
    "@typescript-eslint/parser": "1.4.2",
    "eslint": "5.9.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "4.0.0",
    "eslint-friendly-formatter": "4.0.1",
    "eslint-loader": "2.1.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-restricted-globals": "0.1.1",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "24.0.0",
    "ts-loader": "5.3.3",
    "ts-node": "8.0.3",
    "tslib": "^1.10.0",
    "typescript": "3.3.3333"
  },
  "dependencies": {
    "axios": "^0.16.0",
    "lodash": "^4.17.4",
    "pluralize": "^4.0.0",
    "qs": "^6.4.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "json"
    ],
    "preset": "ts-jest",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "rootDir": "./src"
  }
}
